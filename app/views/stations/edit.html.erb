<h1>Edit Station</h1>

<%= form_for @station do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :lat %>
  <%= f.text_field :lat %>

  <%= f.label :long %>
  <%= f.text_field :long %>

  <%= f.label :dock_count %>
  <%= f.text_field :dock_count %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :installation_date %>
  <%= f.text_field :installation_date %>

  <%= f.submit %>

<% end %>
