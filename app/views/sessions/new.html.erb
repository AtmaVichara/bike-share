<div class="container forms">
  <%= form_tag login_path do %>
    <div class="form-group">
      <%= label_tag :email%>
      <%= text_field_tag :email, "", class: "form-control" %>
    </div>
    <div class="form-group">
      <%= label_tag :password %>
      <%= password_field_tag :password, "", class: "form-control" %>
    </div>
    <%= submit_tag "LogIn", class: 'btn btn-default btn-lg btn-submit' %>
    <%= link_to "Create Account", new_user_path, class: 'create-account' %>
  <% end %>
</div>
